{
  "family": "christanyk-flask-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "flask-xray-app",
      "image": "255945442255.dkr.ecr.us-east-1.amazonaws.com/christanyk-ecr-private-repository:latest",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::255945442255:role/christanyk-ecs-xray-taskexecutionrole",
  "taskRoleArn": "arn:aws:iam::255945442255:role/christanyk-ecs-xray-taskrole"
}
